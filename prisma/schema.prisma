generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model GiftIdea {
  id         Int       @id @default(autoincrement())
  name       String
  price      Decimal
  link       String
  createdAt  DateTime  @default(now())
  updatedAt  DateTime  @default(now())
  archivedAt DateTime?
}
